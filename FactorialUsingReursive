#include <stdio.h>
long long factorial(int num) {
    if (num == 0 || num == 1) {
        return 1;
    }
    return num * factorial(num - 1); // Recursive 
}

int main() {
    int number;
    printf("Enter a number: ");
    scanf("%d", &number);
       unsigned long long result = factorial(number);
        printf("Factorial of %d is: %llu\n", number, result);
    

    return 0;
}

