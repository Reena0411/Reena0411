#include <stdio.h>
#include <string.h>
int main(){
    int i, j = 0;
    char str[100],newstr[100]; 
    scanf("%s",str);
    for (i = 0; str[i] != '\0'; i++) {
        if (str[i] != 'a' && str[i] != 'e' && str[i] != 'i' && str[i] != 'o' && str[i] != 'u' &&
            str[i] != 'A' && str[i] != 'E' && str[i] != 'I' && str[i] != 'O' && str[i] != 'U') {
            newstr[j++] = str[i]; 
        }
    }
    newstr[j] = '\0';  
    printf("%s",newstr);
}

